    "@types/markdown-it": "^14.1.2",
    "@types/sanitize-html": "^2.13.0",
    "astro": "^5.3.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.4.3",
    "markdown-it": "^14.1.0",
    "markdownlint-cli": "^0.44.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sanitize-html": "^2.14.0",
    "tailwindcss": "^4.0.8"
  }
