---
import siteConfig from '@/data/site-config'

const { appletouch, favicons } = siteConfig
---

{
  appletouch.map((i) => (
    <link
      rel="apple-touch-icon-precomposed"
      sizes={`${i}x${i}`}
      href={`/icons/apple-touch-icon-${i}x${i}.png`}
    />
  ))
}

{
  favicons.map((i) => (
    <link
      rel="icon"
      type="image/png"
      sizes={`${i}x${i}`}
      href={`/icons/favicon-${i}x${i}.png`}
    />
  ))
}
