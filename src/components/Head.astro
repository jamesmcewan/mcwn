---
import Meta from "./Meta.astro";
import Icons from "./Icons.astro";

const { content, site } = Astro.props;
---

<Meta description={site.description} themeColor={site.themeColor} />
<title>{content.title} | {site.title}</title>

<Icons favicons={site.favicons} />

<style global>
  @import "../styles/global.css";
</style>
